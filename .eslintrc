{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-new": "warn",
    "no-unused-vars": "warn",
    "quotes": ["error", "single"],
    // "arrow-parens": "off",
    // "eol-last": 0,
    // "linebreak-style": 0,
    // "require-jsdoc" : "off",
    // "semi": "off"
  },
  "plugins": ["jest"],
  "extends": ["airbnb"]
}